// Rules for Stylint 1.3.5 (December 7, 2015)
// Rules: https://github.com/rossPatton/stylint#options, NOT http://rosspatton.github.io/stylint#options
{
  "blocks": { "error": true, "expect": "never" },
  "brackets": { "error": true, "expect": "never" },
  "colons": { "error": true, "expect": "never" },
  "colors": { "error": false, "expect": false },
  "commaSpace": { "error": true, "expect": "always" },
  "commentSpace": { "error": true, "expect": "always" },
  "cssLiteral": { "error": true, "expect": "never" },
  "depthLimit": { "error": true, "expect": 3 },
  "duplicates": { "error": true, "expect": true },
  "efficient": { "error": true, "expect": "always" },
  "extendPref": { "error": true, "expect": "@extend" },
  "globalDupe": { "error": true, "expect": true },
  "indentPref": { "error": true, "expect": false },
  "leadingZero": { "error": true, "expect": "always" },
  "maxErrors": { "error": true, "expect": false },
  "maxWarnings": { "error": true, "expect": false },
  "mixed": { "error": true, "expect": true },
  "namingConvention": { "error": true, "expect": "camelCase" },
  "namingConventionStrict": { "error": true, "expect": false },
  "none": { "error": true, "expect": false },
  "noImportant": { "error": false, "expect": false },
  "parenSpace": { "error": true, "expect": "never" },
  "placeholders": { "error": false, "expect": false },
  "prefixVarsWithDollar": { "error": true, "expect": "always" },
  "quotePref": { "error": true, "expect": "single" },
  "semicolons": { "error": true, "expect": "never" },
  "sortOrder": { "error": false, "expect": false },
  "stackedProperties": { "error": true, "expect": "never" },
  "trailingWhitespace": { "error": true, "expect": "never" },
  "universal": { "error": false, "expect": "never" },
  "valid": { "error": true, "expect": true },
  "zeroUnits": { "error": true, "expect": "never" },
  "zIndexNormalize": { "error": false, "expect": false }
}